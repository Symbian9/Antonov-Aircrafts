<PropertyList>

<!-- Brake Parachute -->

	<path>Main-Parachute.ac</path>

	<offsets>
		<x-m>0.0</x-m>
		<y-m>0.0</y-m>
		<z-m>0.0</z-m>
		<pitch-deg>0</pitch-deg>
	</offsets>

	<animation>
		<type>scale</type>
		<x-offset>0.05</x-offset>
                <y-offset>0.6</y-offset>
                <z-offset>0.6</z-offset>                
	</animation>
	
	<animation>
		<type>select</type>
		<condition>            
	          <equals>
                   <property>sim/multiplay/generic/int[3]</property>
                   <value>1</value>
                  </equals>                    
                 </condition>
	</animation>

	

</PropertyList>
