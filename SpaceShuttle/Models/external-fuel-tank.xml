<PropertyList>
	<path>et.ac</path>
		
<offsets>          <heading-deg>-180</heading-deg> </offsets>  

<animation>
<type>select</type>
 
 <condition>
  <and>
    <equals>
     <property>controls/shuttle/external-fuel-tank</property>
     <value>1</value>
     </equals>
    
     <greater-than>
     <property>consumables/fuel/tank[0]/level-lbs</property>
     <value>10</value>
     </greater-than>
      </and>
 </condition>
 
</animation>

</PropertyList>



