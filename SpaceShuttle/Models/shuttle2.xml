<animation>
<type>pick</type>
<object-name>spaceshutt_spstob</object-name>
<object-name>spaceshutt_spstob.001</object-name>

<visible>true</visible>
<action>
<button>0</button>


<binding>
<command>property-toggle</command>
<property>/controls/shuttle/door/position</property>
<min>0</min>
<max>1</max>
</binding>

<binding>
<command>property-interpolate</command>
<property>sim/multiplay/generic/float[3]</property>
<property>/controls/shuttle/door/position</property> <!-- the value to interpolate to, is 1 -->
<rate>0.15</rate><!-- 8 seconds to 0 from 1 or 1 to 0 -->
</binding>
</action>
</animation>

 
 <animation>
  <type>rotate</type>
  <object-name>spaceshutt_spstob</object-name>
  <property>sim/multiplay/generic/float[3]</property>
  <interpolation>
    <entry>
      <ind>0.2</ind>
      <dep>0</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>-171</dep>
    </entry>
  </interpolation>
  <center>
   <x-m> -2.2671</x-m>
   <y-m> -2.552</y-m>
   <z-m>  -1.52</z-m>
   
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

