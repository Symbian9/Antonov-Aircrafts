<PropertyList>
 <path>fuel1_R1.ac</path>


 
 <animation>
  <type>rotate</type>
  <object-name>rect1</object-name>
  <property>IL-76/systems/fuel/ind/eng_1/needle_1</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>/0</dep></entry> 
   <entry><ind> 5000</ind><dep>  53</dep></entry>
   <entry><ind> 10000</ind><dep>  106</dep></entry>
   <entry><ind> 15000</ind><dep>  160</dep></entry>
   <entry><ind> 20000</ind><dep>  193</dep></entry>
   <entry><ind> 25000</ind><dep>  246</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
  <animation>
  <type>rotate</type>
  <object-name>rect2</object-name>
  <property>IL-76/instrumentation/R2_fuel/L1</property>
  <interpolation>
    <entry><ind>  0.0</ind><dep>  0   </dep></entry> 
   <!--entry><ind> 5000</ind><dep>  62</dep></entry>
   <entry><ind> 10000</ind><dep> 124</dep></entry>
   <entry><ind> 15000</ind><dep> 185</dep></entry-->
   <entry><ind> 25000</ind><dep> 309</dep></entry>

  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation> 
 
 <animation>
  <type>pick</type> 
  <object-name>k1</object-name> 
- <action>
  <button>3</button> 
  <repeatable>true/</repeatable> 
  <interval-sec>0.2</interval-sec>
- <binding>
  <command>property-adjust</command> 
  <property>/IL-76/instrumentation/R2_fuel/k1</property>
  <step>100</step>
  <min>0</min>
  <max>25000</max> 
   </binding>
  </action>
  </animation>
  
   <animation>
  <type>pick</type> 
  <object-name>k1</object-name> 
- <action>
  <button>4</button> 
  <repeatable>true/</repeatable> 
  <interval-sec>0.2</interval-sec>
- <binding>
  <command>property-adjust</command> 
  <property>/IL-76/instrumentation/R2_fuel/k1</property>
  <step>-100</step>
  <min>0</min>
  <max>25000</max> 
   </binding>
  </action>
  </animation>

 
 
 
               <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>group</object-name>
   <emission>
	<red-prop>/IL-76/instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>rect1</object-name>
   <emission>
	<red-prop>/IL-76/instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>rect2</object-name>
   <emission>
	<red-prop>/IL-76/instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>


 </PropertyList>

