<PropertyList>
 <path>fuel1_knob.ac</path>


 
 <animation>
  <type>rotate</type>
  <object-name>k1</object-name>
  <property>systems/fuel/controls/F_ind</property>
  <interpolation>
   <entry><ind>  0</ind><dep>0</dep></entry> 
   <entry><ind> 1</ind><dep> 30</dep></entry>
   <entry><ind> 2</ind><dep> 90</dep></entry>
   <entry><ind> 3</ind><dep> 120</dep></entry>

  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 
 <animation>
  <type>pick</type> 
  <object-name>k1</object-name> 
- <action>
  <button>3</button> 
  <repeatable>true/</repeatable> 
  <interval-sec>0.2</interval-sec>
- <binding>
  <command>property-adjust</command> 
  <property>systems/fuel/controls/F_ind</property>
  <step>1</step>
  <min>0</min>
  <max>3</max> 
   </binding>
  </action>
  </animation>
  
   <animation>
  <type>pick</type> 
  <object-name>k1</object-name> 
- <action>
  <button>4</button> 
  <repeatable>true/</repeatable> 
  <interval-sec>0.2</interval-sec>
- <binding>
  <command>property-adjust</command> 
  <property>systems/fuel/controls/F_ind</property>
  <step>-1</step>
  <min>0</min>
  <max>3</max> 
   </binding>
  </action>
  </animation>

 
 
               <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>group</object-name>
   <emission>
	<red-prop>instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>k1</object-name>
   <emission>
	<red-prop>instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>


 </PropertyList>

