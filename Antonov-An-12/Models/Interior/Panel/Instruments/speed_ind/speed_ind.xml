<PropertyList>
 <path>speed_ind.ac</path>


 
 <animation>
  <type>rotate</type>
  <object-name>kus_rect1</object-name>
   
       <property>velocities/airspeed-kt</property>
         
   <interpolation>
      <entry><ind>   0.00 </ind><dep>   6 </dep></entry>
      <entry><ind> 100</ind><dep>  65</dep></entry>
      <entry><ind> 405.00 </ind><dep> 354 </dep></entry>
    </interpolation>
  
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>kus_rect2</object-name>
  <property>velocities/groundspeed-kt</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>  0</dep></entry>   
   <entry><ind> 200</ind><dep>  7</dep></entry>   
   <entry><ind> 500</ind><dep> 350</dep></entry>
   

  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
               <!--Lights-->
               
   <animation>
   <type>material</type>
   <object-name>kus</object-name>
   <object-name>kus_rect2</object-name>
   <object-name>kus_rect1</object-name>
   <emission>
	<red-prop>instrumentation/lights/panelR/int-red</red-prop>
    <green-prop>instrumentation/lights/panelR/int-green</green-prop>
    <blue-prop>instrumentation/lights/panelR/int-blue</blue-prop>
	</emission>
</animation>              
                                   



 </PropertyList>

