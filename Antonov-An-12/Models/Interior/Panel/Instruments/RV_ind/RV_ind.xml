<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>
 <path>RV_ind.ac</path>

 <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>rect</object-name>
   <emission>
		<factor-prop>tu154/switches/gauge-light</factor-prop>
		<red>0.5</red>
		<green>0.0</green>
		<blue>0.0</blue>
		
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>rect2</object-name>
   <emission>
		<factor-prop>tu154/switches/gauge-light</factor-prop>
		<red>0.4</red>
		<green>0.0</green>
		<blue>0.0</blue>
		
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>group</object-name>
   <emission>
		<factor-prop>tu154/switches/gauge-light</factor-prop>
		<red>0.5</red>
		<green>0.25</green>
		<blue>0.25</blue>
		
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>L1</object-name>
   <emission>
		<factor-prop>instrumentation/rv-um/lamp</factor-prop>
		<red>1.0</red>
		<green>1.0</green>
		<blue>0.0</blue>
		
	</emission>
</animation>

 <!--Instrument-->
 
 <animation>
  <type>rotate</type>
  <object-name>rect</object-name>
  <property>position/gear-agl-m</property>
  <min>0</min>
  <max>750</max> 
 
  <interpolation>
   <entry><ind>  0</ind><dep>  0</dep></entry> 
   <entry><ind> 60</ind><dep>  90</dep></entry>
   <entry><ind> 100</ind><dep>  150</dep></entry>
   <entry><ind>  600</ind><dep> 270</dep></entry> 
   <entry><ind> 750</ind><dep>  300</dep></entry>
  
  
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>rect2</object-name>
  <property>instrumentation/rv-um/control</property>
  <min>0</min>
  <max>9</max> 
 
  <interpolation>
   <entry><ind> 0</ind><dep>  0</dep></entry> 
   <entry><ind> 1</ind><dep> 30</dep></entry>
   <entry><ind> 2</ind><dep> 60</dep></entry>
   <entry><ind> 3</ind><dep> 90</dep></entry> 
   <entry><ind> 4</ind><dep>120</dep></entry>
   <entry><ind> 5</ind><dep>150</dep></entry> 
   <entry><ind> 6</ind><dep>197</dep></entry>
   <entry><ind> 7</ind><dep>245</dep></entry> 
   <entry><ind> 8</ind><dep>295</dep></entry>
   <entry><ind> 9</ind><dep>305</dep></entry> 
  
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 
 <animation>
  <type>pick</type> 
  <object-name>r1</object-name> 
- <action>
  <button>0</button> 
  <repeatable>false/</repeatable> 
  
- <binding>
  <command>property-adjust</command> 
  <property>instrumentation/rv-um/control</property>
  <step>1</step>
  <min>0</min>
  <max>9</max> 
 

  </binding>
  </action>
  </animation>
  
  <animation>
  <type>pick</type> 
  <object-name>r1</object-name> 
- <action>
  <button>1</button> 
  <repeatable>false/</repeatable> 
  
- <binding>
  <command>property-adjust</command> 
  <property>instrumentation/rv-um/control</property>
  <step>-1</step>
  <min>0</min>
  <max>9</max> 
 

  </binding>
  </action>
  </animation>


 
 </PropertyList>

