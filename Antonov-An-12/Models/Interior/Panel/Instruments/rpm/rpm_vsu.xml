<PropertyList>
 <path>rpm_vsu.ac</path>


 
 <animation>
  <type>rotate</type>
  <object-name>rect1</object-name>
  <property>IL-76/systems/apu/rpm-norm</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>/0</dep></entry> 
   <entry><ind> 30</ind><dep>  90</dep></entry>
   <entry><ind> 60</ind><dep>  180</dep></entry>
   <entry><ind> 90</ind><dep>  272</dep></entry>
   <entry><ind> 110</ind><dep>  345</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 

 
               <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>group</object-name>
   <emission>
		<red>0.5</red>
		<green>0.0</green>
		<blue>0.0</blue>
		<factor-prop>systems/electrical/outputs/nav-lights</factor-prop>
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>rect1</object-name>
   <emission>
		<red>0.6</red>
		<green>0.0</green>
		<blue>0.0</blue>
		<factor-prop>systems/electrical/outputs/nav-lights</factor-prop>
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>rect2</object-name>
   <emission>
		<red>0.6</red>
		<green>0.0</green>
		<blue>0.0</blue>
		<factor-prop>systems/electrical/outputs/nav-lights</factor-prop>
	</emission>
</animation>


 </PropertyList>

