<PropertyList>
 <path>rpm.ac</path>

 
 
 <animation>
  <type>rotate</type>
  <object-name>rect1</object-name>
  <property>engines/engine/n2</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>/0</dep></entry> 
   <entry><ind> 30</ind><dep>  90</dep></entry>
   <entry><ind> 60</ind><dep>  180</dep></entry>
   <entry><ind> 90</ind><dep>  270</dep></entry>
   <entry><ind> 110</ind><dep>  340</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>rect2</object-name>
  <property>engines/engine/rpm</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>/0</dep></entry>   
   <entry><ind> 999</ind><dep>  325</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
               <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>group</object-name>
   <emission>
	<red-prop>instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>rect1</object-name>
   <emission>
	<red-prop>instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>rect2</object-name>
   <emission>
	<red-prop>instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>


 </PropertyList>

